      <nz-transfer *nzSpaceItem [nzDataSource]="[]" nzStatus="warning" nzShowSearch></nz-transfer>
    </nz-space>
  `<% } else { %>templateUrl: './<%= dasherize(name) %>.component.html'<% } %>
})
